<dict>
	<key>IC_CheckMinimumAvailableDiskSpace</key>
	<string>	function IC_CheckMinimumAvailableDiskSpace(inMinimumAvailableSpaceKB)
	{
		return (my.target.availableKilobytes>=inMinimumAvailableSpaceKB);
	}</string>
</dict>
